{
	"manifest_version": 2,
	"name": "EmotiClean",
	"version": "1.0.0",
	"description": "Replace bad words with emoji!",
	"icons": { "128": "icon.png" },
	"background": {
		"page": "background.html",
		"persistent": false
	},
	"content_scripts": [
		{
		"matches": ["http://*/*"],
		"js": ["filter.js"],
		"run_at": "document_end",
		"all_frames": true
		}
	],
	"permissions": [
		"tabs"
	]
}
